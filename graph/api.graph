}

query DoelById($id:ID) {
  Doel(id:$id) {
    id
    title
    description
    bron
    vakbegrippen
    Leerlingtekst {
      ...Leerlingtekst
    }
    
    Doelniveau {
      id
      prefix
      ce_se
      Niveau {
