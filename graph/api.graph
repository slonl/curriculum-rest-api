query DoelenOpNiveauByVakleergebiedById($id:ID, $niveau:ID) {
  LpibVakleergebied(id:$id) {
    id
    title
    Niveau(filter:{id:$niveau}) {
      id
      title
    }
    LpibVakkern {
      id
      title
      LpibVaksubkern {
        id
        title
        LpibVakinhoud {
          id
          title
          Doelniveau(filter:{niveau_id:[$niveau]}) {
            ...Doelen
          }
        }
        Doelniveau(filter:{niveau_id:[$niveau]}) {
          ...Doelen
        }
      }
      LpibVakinhoud {
        id
        title
        Doelniveau(filter:{niveau_id:[$niveau]}) {
          ...Doelen
        }
      }
      Doelniveau(filter:{niveau_id:[$niveau]}) {
        ...Doelen
      }
    }
  }
}

