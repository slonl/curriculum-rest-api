      id
      title
      description
      vakbegrippen
      bron
      Leerlingtekst {
        title
        description
      }     
    }
    Niveau {
      id
      title
      description
      prefix
      type
    }
    Kerndoel {
      id
      title
      description
      kerndoelLabel
      prefix
    }
    ExamenprogrammaDomein {
      id
      title
      prefix
    }
    ExamenprogrammaSubdomein {
      id
      title
      prefix
    }
    ExamenprogrammaEindterm {
      id
      title
      prefix
    }
}
fragment Doelen on Doelniveau {
    id
    Doel {
      id
      title
      description
      bron
      vakbegrippen
      Leerlingtekst {
        title
        description
      }     
    }
    Kerndoel {
      id
      title
      description
      kerndoelLabel
      prefix
    }
}
fragment Niveau on Niveau {
   id
   title
   description
   prefix
   type
}
fragment NiveauShort on Niveau {
    id
    title
    prefix
}
fragment Leerlingtekst on Leerlingtekst {
    title
    description
}
fragment LdkVakkern on LdkVakkern {
    id
    title
    prefix
}
fragment LdkVaksubkern on LdkVaksubkern {
    id
    title
    prefix
}
fragment LdkVakinhoud on LdkVakinhoud {
